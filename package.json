{
  "name": "electron-vuetify-js",
  "version": "0.0.0",
  "description": "electron-vuetify-js",
  "author": "WangJianHui",
  "scripts": {
    "start": "nodemon --watch src/electron --exec \"npm run app:dev\"",
    "dev": "vite",
    "build": "vite build && electron-builder",
    "preview": "vite preview",
    "app:dev": "concurrently \"vite\" \"wait-on http://localhost:3033 && cross-env VITE_DEV_SERVER_URL=http://localhost:3033 cross-env NODE_ENV=development electron .\"",
    "app:build": "npm run copy-native && vite build && electron-builder --config electron-builder.json --dir",
    "app:make": "npm run copy-native && vite build && electron-builder --config electron-builder.json --win dir",
    "app:make:portable": "npm run copy-native && vite build && electron-builder --config electron-builder.json --win portable",
    "set:npm:official": "npm config set registry https://registry.npmjs.org/",
    "set:npm:huawei": "npm config set registry https://mirrors.huaweicloud.com/repository/npm/",
    "gyp": "node-gyp rebuild",
    "copy-native": "copyfiles -u 1 \"build/Release/*.node\" dist/",
    "lint": "eslint . --fix --ignore-path .gitignore",
    "clean": "rimraf dist dist-electron release .eslintrc-auto-import.json auto-imports.d.ts components.d.ts typed-router.d.ts"
  },
  "dependencies": {
    "@mdi/font": "7.4.47",
    "core-js": "^3.37.1",
    "electron-window-manager": "^1.0.6",
    "vue": "^3.4.31",
    "vuetify": "^3.6.11"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/eslint-config-typescript": "^14.1.1",
    "concurrently": "^9.0.1",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "electron": "^33.0.1",
    "electron-builder": "^24.13.3",
    "eslint": "^8.57.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-config-vuetify": "^1.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.4.0",
    "eslint-plugin-vue": "^9.27.0",
    "nodemon": "^3.1.7",
    "pinia": "^2.1.7",
    "rimraf": "^6.0.1",
    "sass": "1.77.6",
    "unplugin-auto-import": "^0.17.6",
    "unplugin-fonts": "^1.1.1",
    "unplugin-vue-components": "^0.27.2",
    "unplugin-vue-router": "^0.10.0",
    "vite": "^5.3.3",
    "vite-plugin-electron": "^0.28.8",
    "vite-plugin-vue-layouts": "^0.11.0",
    "vite-plugin-vuetify": "^2.0.3",
    "vue-router": "^4.4.0"
  },
  "main": "src/electron/main.js"
}
