{
  "name": "electron-vuetify-js",
  "version": "0.0.1",
  "description": "electron-vuetify-js",
  "author": "WangJianHui",
  "scripts": {
    "start": "chcp 65001 && nodemon --watch src/electron --exec vite",
    "dev": "chcp 65001 && vite",
    "build": "chcp 65001 && vite build && electron-builder",
    "preview": "chcp 65001 && vite preview",
    "make:win": "chcp 65001 && vite build && electron-builder --config electron-builder.json --win dir && npm run copy-assets",
    "make": "chcp 65001 && vite build && electron-builder --config electron-builder.json --win dir && npm run copy-assets",
    "make:portable": "chcp 65001 && vite build && electron-builder --config electron-builder.json --win portable && npm run copy-assets",
    "set:npm:official": "npm config set registry https://registry.npmjs.org/",
    "set:npm:huawei": "npm config set registry https://mirrors.huaweicloud.com/repository/npm/",
    "set:yarn:offical": "yarn config set registry https://registry.npmjs.org/",
    "set:yarn:huawei": "yarn config set registry https://mirrors.huaweicloud.com/repository/npm/",
    "set:yarn:taobao": "yarn config set registry https://registry.npm.taobao.org/",
    "gyp": "node-gyp rebuild",
    "copy-assets": "copyfiles -u 1 \"assets/**/*\" release/0.0.1/win-unpacked/",
    "lint": "eslint . --fix --ignore-path .gitignore",
    "clean": "rimraf dist dist-electron release .eslintrc-auto-import.json auto-imports.d.ts components.d.ts typed-router.d.ts && rimraf --glob \"vite.config.mjs.timestamp-*\"",
    "prettier": "npx prettier --check .",
    "prettier:fix": "npx prettier --write ."
  },
  "dependencies": {
    "@mdi/font": "7.4.47",
    "core-js": "^3.37.1",
    "electron-window-manager": "^1.0.6",
    "koffi": "^2.9.1",
    "vue": "^3.4.31",
    "vuetify": "^3.6.11"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.5",
    "concurrently": "^9.0.1",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "electron": "^33.0.1",
    "electron-builder": "^24.13.3",
    "eslint": "^8.57.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-config-vuetify": "^1.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.4.0",
    "eslint-plugin-vue": "^9.27.0",
    "nodemon": "^3.1.7",
    "pinia": "^2.1.7",
    "rimraf": "^6.0.1",
    "sass": "1.77.6",
    "unplugin-auto-import": "^0.17.6",
    "unplugin-fonts": "^1.1.1",
    "unplugin-vue-components": "^0.27.2",
    "unplugin-vue-router": "^0.10.0",
    "vite": "^5.3.3",
    "vite-plugin-electron": "^0.28.8",
    "vite-plugin-vue-layouts": "^0.11.0",
    "vite-plugin-vuetify": "^2.0.3",
    "vue-router": "^4.4.0"
  },
  "main": "src/electron/main.js"
}
